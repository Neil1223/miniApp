<view class="container">
  <view class="img" k:if="{{count > 20}}">count > 20</view>
  <view class="img" k:elif="{{count > 10}}">count > 10</view>
  <view class="img" k:elif="{{count > 5}}">count > 5</view>
  <view class="img" k:else>count <= 20</view>
  <view class="desc">以下将展示小程序官方组件能力，组件样式仅供参考，开发者可根据自身需求自定义组件样式，具体属性参数详见文档: https://www.kiple.com</view>
  <view class="panel" k:for="{{ list }}">
    <view class="panel-h">
      <text class="panel-text">{{index}}-{{item.name}}</text>
      <view k:for="{{item.pages}}" k:for-index="idx">
        <view>{{idx}}-{{item}}</view>
      </view>
    </view>
  </view>
</view>
